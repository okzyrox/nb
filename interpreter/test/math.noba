fn add(a: int, b: int) {
    return a + b
}

fn main() {
    var w = 2 + 2
    var x = 5 - 2
    var y = 3 * 4
    var z = 4 / 2

    println(w) //3
    println(x) // 5?

    println(y) // 12
    println(z) // 2

    assert(w, 4, "w should be 4")
    assert(x, 3, "x should be 3")

    var n = add(1, 2)
    println(n)
    assert(n, 3, "n should be 3")

}